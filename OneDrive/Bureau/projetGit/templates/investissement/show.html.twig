{% extends 'base-front.html.twig' %}

{% block title %}Investissement{% endblock %}

{% block body %}
    <h1>Investissement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ investissement.id }}</td>
            </tr>
            <tr>
                <th>DateAchat</th>
                <td>{{ investissement.dateAchat ? investissement.dateAchat|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>PrixAchat</th>
                <td>{{ investissement.prixAchat }}</td>
            </tr>
            <tr>
                <th>ROI</th>
                <td>{{ investissement.ROI }}</td>
            </tr>
            <tr>
                <th>Montant</th>
                <td>{{ investissement.montant }}</td>
            </tr>
            <tr>
                <th>Tax</th>
                <td>{{ investissement.tax }}</td>
            </tr>
            <tr>
                <th>ReId</th>
                <td>{{ investissement.reId }}</td>
            </tr>
            <tr>
                <th>UserId</th>
                <td>{{ investissement.userId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_investissement_index') }}">back to list</a>

    <a href="{{ path('app_investissement_edit', {'id': investissement.id}) }}">edit</a>

    {{ include('investissement/_delete_form.html.twig') }}
{% endblock %}
